{
  "name": "@timescope/solid",
  "private": true,
  "type": "module",
  "description": "Timescope component for SolidJS",
  "keywords": [
    "solid",
    "solidjs"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "vite",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "solid": "./dist/index.jsx",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@kikuchan/decimal": "0.1.0-alpha.5",
    "solid-js": "^1.9.9"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@kikuchan/decimal": "0.1.0-alpha.5",
    "@solidjs/testing-library": "^0.8.10",
    "@types/node": "^25.0.3",
    "@vitest/browser-playwright": "^4.0.16",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-solid": "^0.14.5",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "rolldown-plugin-solid": "^0.2.1",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "timescope": "workspace:*"
  }
}
