{
  "name": "@timescope/svelte",
  "private": true,
  "type": "module",
  "description": "Timescope component for Svelte",
  "keywords": [
    "svelte"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsdown",
    "format": "prettier --check .",
    "lint": "prettier --check . && eslint ."
  },
  "sideEffects": [
    "**/*.css"
  ],
  "types": "./src/lib/index.ts",
  "main": "./src/lib/index.ts",
  "exports": {
    ".": {
      "types": "./src/lib/index.ts",
      "svelte": "./src/lib/index.ts"
    }
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^25",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-svelte": "^3.4.1",
    "publint": "^0.3.16",
    "svelte": "^5.46.1",
    "svelte-preprocess": "^6.0.3",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "timescope": "workspace:*"
  }
}
